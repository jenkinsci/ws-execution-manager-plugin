<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry field="request" title="Request">
        <f:combobox/>
    </f:entry>
    <f:entry title="Or">
    </f:entry>
    <f:entry field="bookmark" title="Bookmark">
        <f:combobox/>
    </f:entry>

    <f:advanced title="Alternative Execution Manager">
        <f:property field="altEMConfig"></f:property>
    </f:advanced>

    <f:block>
        <div style="margin-left: 1em;">
            <table width="100%">

                <div>
                    <h5>Additional options:</h5>
                </div>

                <!--f:optionalProperty field="altEMConfig" title="Alternative Execution Manager"></f:optionalProperty-->

                <f:optionalBlock field="execParams" title="Parameters" checked="${instance.execParamsEnabled}">
                    <f:entry>
                        <f:repeatableProperty field="execParamList" header=""/>
                        <div><br/>See list of
                            <a href="${rootURL}/env-vars.html" target="_blank">available Jenkins environment variables
                            </a>
                        </div>
                    </f:entry>
                </f:optionalBlock>

                <f:optionalProperty field="waitConfig" title="Wait Configuration"></f:optionalProperty>

            </table>
        </div>
    </f:block>
</j:jelly>